<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="com.eventmoa.app.user.vo.userVO" alias="userVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.point_boardVO" alias="point_boardVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.freeTalk_boardVO" alias="freeTalk_boardVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.freeTalk_filesVO" alias="freeTalk_filesVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.freeTalk_ReplyVO" alias="freeTalk_ReplyVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.event_boardVO" alias="event_boardVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.event_filesVO" alias="event_filesVO"/>
		<typeAlias type="com.eventmoa.app.board.vo.event_replyVO" alias="event_replyVO"/>
	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC"/>
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.OracleDriver"/>
				<property name="url" value="jdbc:oracle:thin:@localhost:1515/XE"/>
				<property name="username" value="eventmoa"/>
				<property name="password" value="0008"/>
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="com/eventmoa/mybatis/sql/user.xml"/>
		<mapper resource="com/eventmoa/mybatis/sql/freeTalk_board.xml"/>
		<mapper resource="com/eventmoa/mybatis/sql/freeTalk_files.xml"/>
	</mappers>
</configuration>